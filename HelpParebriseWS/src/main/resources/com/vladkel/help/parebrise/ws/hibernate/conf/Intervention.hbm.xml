<?xml version="1.0"?>
	<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
	<hibernate-mapping>
		<class name="Intervention" table="intervention">
			
			<id name="indice_intervention" type="int" column="indice_intervention">
				<generator class="native"/>
			</id>
			
			<property name="indice_intervention" type="int" not-null="true" />
			<property name="indice_client" type="int" not-null="true" />
			<property name="indice_vehicule" type="int" not-null="true" />
			<property name="date_intervention" type="string" not-null="true" />
			<property name="date_facture" type="string" not-null="true" />
			<property name="numero_facture" type="string" not-null="true" />
			<property name="prix_HT" type="Double" not-null="true" />
			<property name="prix_TTC" type="Double" not-null="true" />
			<property name="id_tva" type="int" not-null="true" />
			<property name="acompte" type="Double" not-null="true" />
			<property name="remise" type="Double" not-null="true" />
			<property name="franchise" type="Double" not-null="true" />
			<property name="indice_prestation" type="int" not-null="true" />
			<property name="indice_mode_paiement" type="int" not-null="true" />
			<property name="date_sinistre" type="string" not-null="true" />
			<property name="cause_sinistre" type="string" not-null="true" />
			<property name="adresse_intervention" type="string" not-null="true" />
			<property name="bon_de_commande" type="string" not-null="true" />
			<property name="date_echeance" type="string" not-null="true" />
			<property name="indice_contact" type="int" not-null="true" />
			<property name="assurance_impression" type="string" not-null="true" />

		</class>
	</hibernate-mapping>